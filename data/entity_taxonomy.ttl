@prefix dct: <http://purl.org/dc/terms/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix : <https://github.com/graphsense/graphsense-tagpacks/vocab/entities/> .

<https://github.com/graphsense/graphsense-tagpacks/vocab/abuse/>
    a skos:ConceptScheme ;
    dct:title "Entity Types" ;
    dct:creator "Interpol Darknet and Cryptocurrencies Working Group" ;
    skos:hasTopConcept :Actor, :Service .

:Actor
    a skos:Concept ;
    skos:definition "Some very generic example definition." ;
    skos:inScheme <https://github.com/graphsense/graphsense-tagpacks/vocab/abuse/> ;
    skos:narrower :Organization, :User ;
    skos:prefLabel "Actor"@en ;
    skos:topConceptOf <https://github.com/graphsense/graphsense-tagpacks/vocab/abuse/> .

:Exchange
    a skos:Concept ;
    skos:broader :Service ;
    skos:inScheme <https://github.com/graphsense/graphsense-tagpacks/vocab/abuse/> ;
    skos:prefLabel "Exchange"@en .

:FileSharing
    a skos:Concept ;
    skos:broader :Service ;
    skos:inScheme <https://github.com/graphsense/graphsense-tagpacks/vocab/abuse/> ;
    skos:prefLabel "File Sharing"@en .

:Index
    a skos:Concept ;
    skos:broader :Service ;
    skos:inScheme <https://github.com/graphsense/graphsense-tagpacks/vocab/abuse/> ;
    skos:prefLabel "Index"@en .

:Market
    a skos:Concept ;
    skos:broader :Service ;
    skos:inScheme <https://github.com/graphsense/graphsense-tagpacks/vocab/abuse/> ;
    skos:prefLabel "Market"@en .

:MessagingService
    a skos:Concept ;
    skos:broader :Service ;
    skos:inScheme <https://github.com/graphsense/graphsense-tagpacks/vocab/abuse/> ;
    skos:prefLabel "Messaging Service"@en .

:MixingService
    a skos:Concept ;
    skos:broader :Service ;
    skos:inScheme <https://github.com/graphsense/graphsense-tagpacks/vocab/abuse/> ;
    skos:prefLabel "Mixing Service"@en .

:Organization
    a skos:Concept ;
    rdfs:seeAlso <http://onlinesource.com/defintions> ;
    skos:broader :Actor ;
    skos:definition "This defintion describes what an organization is supposed to be." ;
    skos:inScheme <https://github.com/graphsense/graphsense-tagpacks/vocab/abuse/> ;
    skos:prefLabel "Organization"@en .

:Search
    a skos:Concept ;
    skos:broader :Service ;
    skos:inScheme <https://github.com/graphsense/graphsense-tagpacks/vocab/abuse/> ;
    skos:prefLabel "Search"@en .

:Service
    a skos:Concept ;
    skos:inScheme <https://github.com/graphsense/graphsense-tagpacks/vocab/abuse/> ;
    skos:narrower :Exchange, :FileSharing, :Index, :Market, :MessagingService, :MixingService, :Search, :Shop, :WalletProvider ;
    skos:prefLabel "Service"@en ;
    skos:topConceptOf <https://github.com/graphsense/graphsense-tagpacks/vocab/abuse/> .

:Shop
    a skos:Concept ;
    skos:broader :Service ;
    skos:inScheme <https://github.com/graphsense/graphsense-tagpacks/vocab/abuse/> ;
    skos:prefLabel "Shop"@en .

:User
    a skos:Concept ;
    skos:broader :Actor ;
    skos:inScheme <https://github.com/graphsense/graphsense-tagpacks/vocab/abuse/> ;
    skos:prefLabel "User"@en .

:WalletProvider
    a skos:Concept ;
    skos:broader :Service ;
    skos:inScheme <https://github.com/graphsense/graphsense-tagpacks/vocab/abuse/> ;
    skos:prefLabel "Wallet Provider"@en .
